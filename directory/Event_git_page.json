{
          "@context": [
              "https://www.w3.org/2018/credentials/v1",
              "https://hirekarma1.github.io/tempfiles/directory/Event_git_page.json",
              "https://w3id.org/security/suites/ed25519-2020/v1"
          ],
          "issuer": "${_issuer}",
          "type": [
              "VerifiableCredential",
              "EventCredential"
          ],
          "issuanceDate": "${validFrom}",
          "expirationDate": "${validUntil}",
          "credentialSubject": {
              "id": "${_holderId}",			
              "event_id": "${event_id}",
              "event_name": "${event_name}",
              "event_date": "${event_date}",
              "gender": "${gender}",
              "organizer": "${organizer}",
              "contact": "${contact}"
          }
     }